      "cell_type": "code",
      "source": [
        "import random\n",
        "def start():\n",
        "  user=input(\"What's your choice \\n Enter (R) for rock, (P) for paper, (S) for scissor: \").lower()\n",
        "  computer=random.choice([\"r\",\"p\",\"s\"])\n",
        "  if user==computer:\n",
        "    return \"Tie\"\n",
        "  if game(user, computer):\n",
        "    print(f\"You are winner\\ncomputer choice was {computer}\")\n",
        "  else:\n",
        "    print(f\"You are loss\\ncomputer choice was {computer}\")\n",
        "def game(user,computer):\n",
        "  if (user==\"r\" and computer==\"s\") or (user==\"p\" and computer==\"r\") or (user==\"s\" and computer==\"p\"):\n",
        "    return True\n",
        "start()"
