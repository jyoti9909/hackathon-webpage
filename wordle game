        "def hangman():\n",
        "  word=(random.choice(li1)).upper()\n",
        "  total_alpha=set(word)\n",
        "  alpha=set(string.ascii_uppercase)\n",
        "  while len(total_alpha)>0:\n",
        "    a=input(\"Guess the alphabet: \").upper()\n",
        "    if a in alpha:\n",
        "      if a in total_alpha:\n",
        "        total_alpha.remove(a)\n",
        "        print(\"Oh! yeh I found it.\")\n",
        "      else:\n",
        "        print(\"No! this is not found.\")\n",
        "    elif a not in alpha:\n",
        "      print(\"This is not found in my alpha list.\")\n",
        "    else:\n",
        "      print(\"This is invalid.\")\n",
        "  return (f\"The word was {word}\")\n",
        "hangman()\n"
