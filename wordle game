        "  # guess a number\n",
        "import random\n",
        "def guessing(x):\n",
        "    number=random.randint(1,x)\n",
        "    guess=0\n",
        "    while guess!=number:\n",
        "      guess=int(input(\"Enter your number: \"))\n",
        "      if guess<number:\n",
